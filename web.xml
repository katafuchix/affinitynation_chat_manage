<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <resource-ref>
      <res-ref-name>jdbc/postgres</res-ref-name>
      <res-type>javax.sql.DataSource</res-type>
      <res-auth>Container</res-auth>
  </resource-ref>


    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>HelloWorld</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/servlet/HelloWorld</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ChatTest</servlet-name>
        <servlet-class>ChatTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChatTest</servlet-name>
        <url-pattern>/servlet/ChatTest</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ChatInput</servlet-name>
        <servlet-class>ChatInput</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChatInput</servlet-name>
        <url-pattern>/servlet/ChatInput</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ChatOutput</servlet-name>
        <servlet-class>ChatOutput</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChatOutput</servlet-name>
        <url-pattern>/servlet/ChatOutput</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ChatScdule</servlet-name>
        <servlet-class>ChatScdule</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChatScdule</servlet-name>
        <url-pattern>/servlet/ChatScdule</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ChatProfile</servlet-name>
        <servlet-class>ChatProfile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChatProfile</servlet-name>
        <url-pattern>/servlet/ChatProfile</url-pattern>
    </servlet-mapping>

